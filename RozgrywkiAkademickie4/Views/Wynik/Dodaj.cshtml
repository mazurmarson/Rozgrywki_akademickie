@model RozgrywkiAkademickie4.Models.Wynik
@{
    ViewData["Title"] = "Dodaj";
}

<h1>Dodaj</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="Dodaj">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Miejsce" class="control-label"></label>
                <input asp-for="Miejsce" class="form-control" />
                <span asp-validation-for="Miejsce" class="text-danger"></span>
            </div>

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Punkty" class="control-label"></label>
                <input asp-for="Punkty" class="form-control" />
                <span asp-validation-for="Punkty" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Dodaj" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


